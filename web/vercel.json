{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "env": {
    "NODE_VERSION": "20",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
